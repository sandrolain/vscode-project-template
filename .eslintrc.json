{
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module",
		"ecmaFeatures": {
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"no-console": "warn",
		"quotes": ["error", "double"],
		"no-inline-comments": "warn",
		"semi": [2, "always"]
	}
}
